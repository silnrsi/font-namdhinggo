lookup VKem {
    lookupflag MarkAttachmentType @VowelsKem;
    # The VKem positioning rule positions the kemphreng on all upper vowels (except ikar, which has its own ligature).
    # The vowel itself is positioned on the consonant with the Akar or EO positioning rule.
    pos mark akar-limb <anchor -261 1285> mark @_K;
    pos mark ekar-limb <anchor -260 1280> mark @_K;
    pos mark okar-limb <anchor -150 1514> mark @_K;
    pos mark subwa_akar-limb <anchor -300 1300> mark @_K;
    pos mark subwa_ekar-limb <anchor -300 1250> mark @_K;
    pos mark subya_akar-limb <anchor -260 1250> mark @_K;
    pos mark subya_ekar-limb <anchor -270 1250> mark @_K;
    # was @MARK_VK
} VKem;

lookup GlideU {
    lookupflag 0;
    # The GlideU positioning rule positions the Ukar on the glides Ya and Wa.
    # (There is already a ligature for each consonant with the Ra+Ukar combination).
    pos base Ohm <anchor 61 1192> mark @_G;
    pos base Omegagreek <anchor 65 1260> mark @_G;
    pos base dottedcircle <anchor 991 0> mark @_G;
    pos base subwa-limb <anchor -133 -40> mark @_G;
    pos base subya-limb <anchor -121 -40> mark @_G;
    # was @MARK_U
} GlideU;

