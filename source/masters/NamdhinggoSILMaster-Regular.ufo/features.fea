    sub la-limb subra-limb ukar-limb by la_subra_ukar-limb;
    sub wa-limb subra-limb ukar-limb by wa_subra_ukar-limb;
    sub sha-limb subra-limb ukar-limb by sha_subra_ukar-limb;
