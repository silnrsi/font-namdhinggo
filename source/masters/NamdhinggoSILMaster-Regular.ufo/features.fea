    pos base [jha-limb jha_subra_ukar-limb jha_ukar-limb ta-limb ta_subra_ukar-limb ta_ukar-limb] <anchor 450 1050> mark @_K;
    pos base [kha-limb kha_subra_ukar-limb kha_ukar-limb pa-limb pa_subra_ukar-limb pa_ukar-limb] <anchor 680 1050> mark @_K;
    pos base [la-limb la_subra_ukar-limb la_ukar-limb] <anchor 585 1050> mark @_K;
    pos base [ma-limb ma_subra_ukar-limb ma_ukar-limb] <anchor 560 1050> mark @_K;
