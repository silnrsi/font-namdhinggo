    sub gyan-limb subra-limb ukar-limb by gyan_subra_ukar-limb;
    sub tra-limb subra-limb ukar-limb by tra_subra_ukar-limb;
} RaUkar;

lookup Ukar {
    lookupflag 0;
    # The Ukar substitution rule replaces Consonant + Ukar with a ligature. It also applies to the Vowel-Carrier, which has its own ligature with ukar.
    sub ka-limb ukar-limb by ka_ukar-limb;
    sub kha-limb ukar-limb by kha_ukar-limb;
    sub ga-limb ukar-limb by ga_ukar-limb;
