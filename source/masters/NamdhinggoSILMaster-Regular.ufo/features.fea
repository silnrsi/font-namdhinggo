    pos base sa-limb <anchor 580 1050> mark @_U;
    pos base sha-limb <anchor 560 1050> mark @_U;
    pos base ssa-limb <anchor 421 1050> mark @_U;
    pos base ta-limb <anchor 450 1050> mark @_U;
